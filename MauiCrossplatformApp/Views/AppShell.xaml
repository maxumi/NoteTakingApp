<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiCrossplatformApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:MauiCrossplatformApp.Views"
    xmlns:local="clr-namespace:MauiCrossplatformApp"
    Shell.FlyoutBehavior="Flyout"
    Title="MauiCrossplatformApp">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Note"
        ContentTemplate="{DataTemplate views:NotePage}" />

</Shell>
